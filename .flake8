[flake8]
exclude =
    venv/*
    *.venv/*
    __pycache__/*
    .git/*
    */migrations/*
require-plugins =
    flake8-bugbear
    flake8-builtins
    flake8-commas
    flake8-comprehensions
    flake8-dunder-all
    flake8-eradicate
    flake8-fixme
    flake8-import-order
    flake8-quotes
    pep8-naming
application-import-names =
    about
    catalog
    core
    download
    feedback
    homepage
    lyceum
import-order-style = google
inline-quotes = single
